                    android:id="@+id/txtCategory"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_71pxh"
                    android:layout_marginStart="@dimen/_71pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{frameOneVM.frameOneModel.txtCategory}"
                    style="@style/txtInterregular32_1" tools:text="@string/lbl_category"/>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtSelectwhichga"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{frameOneVM.frameOneModel.txtSelectwhichga}"
                    style="@style/txtInterregular16" tools:text="@string/msg_select_which_ga"/>
