                        android:layout_height="@dimen/_99pxh"
                        android:layout_width="@dimen/_106pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_ellipse2}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse2"/>
                    <ImageView android:id="@+id/imageRectangleNineteen"
                        android:layout_height="@dimen/_99pxh"
                        android:layout_marginStart="@dimen/_73pxh"
                        android:layout_width="@dimen/_106pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_ellipse3}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse3"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowrectangletwentythree"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_63pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/imageRectangleTwentyThree"
                        android:layout_height="@dimen/_99pxh"
                        android:layout_width="@dimen/_106pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle23}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle23"/>
                    <ImageView
                        android:id="@+id/imageRectangleTwentyFour"
                        android:layout_height="@dimen/_99pxh"
                        android:layout_marginStart="@dimen/_73pxh"
                        android:layout_width="@dimen/_106pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle24}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle24"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowback"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_marginTop="@dimen/_81pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnBack"
                        android:layout_height="@dimen/_45pxv"
                        android:layout_width="@dimen/_110pxh"
                        android:paddingBottom="@dimen/_14pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_14pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_back"
                        android:textAllCaps="false"
                        style="@style/btnSolid_2" tools:text="@string/lbl_back"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnNext"
                        android:layout_height="@dimen/_45pxv"
                        android:layout_marginStart="@dimen/_53pxh"
                        android:layout_width="@dimen/_110pxh"
                        android:paddingBottom="@dimen/_14pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_14pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_next"
                        android:textAllCaps="false"
                        style="@style/btnSolid_3" tools:text="@string/lbl_next"/>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
