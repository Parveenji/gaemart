                    android:id="@+id/linearRowrectanglesixteen"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRectangleSixteen"
                        android:layout_height="@dimen/_99pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_106pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_ellipse1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse1"/>
                    <ImageView android:id="@+id/imageRectangleSeventeen"
                        android:layout_height="@dimen/_99pxh"
                        android:layout_marginStart="@dimen/_73pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_106pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_ellipse1_55x63}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse1_55x63"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowrectangleeighteen"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_62pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
