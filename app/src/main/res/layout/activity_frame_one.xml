                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_black_900_18x15"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_37pxh"
            android:layout_marginTop="@dimen/_29pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnellipse734"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_57pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <View
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_59"
                    android:id="@+id/viewEllipse734"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_84pxv"
                    android:layout_marginEnd="@dimen/_83pxh"
                    android:layout_marginStart="@dimen/_83pxh" android:layout_width="@dimen/_118pxh"/>
