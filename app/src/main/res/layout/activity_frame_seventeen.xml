<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="frameSeventeenVM" type="com.parveensapplication.app.modules.frameseventeen.data.viewmodel.FrameSeventeenVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnvector"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_900">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylebluegray_900">
            <LinearLayout android:id="@+id/linearRowiconmenu"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_33pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowvector"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.87"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_36pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_width="@dimen/_33pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_black_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_black_900"/>
                    <ImageView android:id="@+id/imageVectorOne"
                        android:layout_height="@dimen/_32pxh"
                        android:layout_marginStart="@dimen/_67pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_32pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_amber_900_32x32"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_amber_900_32x32"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtChat"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_weight="0.35"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{frameSeventeenVM.frameSeventeenModel.txtChat}"
                        style="@style/txtIntersemibold15" tools:text="@string/lbl_chat"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageIconmenu"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginStart="@dimen/_94pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconmenu"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconmenu"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearColumnellipse734"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_900">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowellipse734"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <View
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_29_5"
                    android:id="@+id/viewEllipse734"
                    android:layout_height="@dimen/_51pxv" android:layout_width="@dimen/_59pxh"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNameofseller"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.7"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{frameSeventeenVM.frameSeventeenModel.txtNameofseller}"
                    style="@style/txtInterregular20" tools:text="@string/lbl_name_of_seller"/>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearColumnsafetytips"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_288pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_800">
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowsafetytips"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSafetyTips"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.88"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{frameSeventeenVM.frameSeventeenModel.txtSafetyTips}"
                        style="@style/txtInterbold20" tools:text="@string/lbl_safety_tips"/>
                    <ImageView android:id="@+id/imageVectorTwo"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_92pxh"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_black_900_18x15"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_black_900_18x15"/>
                </LinearLayout>
                <TextView android:gravity="start"
                    android:id="@+id/txtDONTtakerisk"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_41pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_width="match_parent"
                    android:text="@{frameSeventeenVM.frameSeventeenModel.txtDONTtakerisk}"
                    style="@style/txtInterregular14" tools:text="@string/msg_don_t_take_risk"/>
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:text="@{frameSeventeenVM.frameSeventeenModel.txtDescription}"
                    style="@style/txtInterregular14" tools:text="@string/msg_don_t_disclose"/>
                <TextView android:gravity="start"
                    android:id="@+id/txtDescriptionOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_37pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent"
                    android:text="@{frameSeventeenVM.frameSeventeenModel.txtDescriptionOne}"
                    style="@style/txtInterregular14" tools:text="@string/msg_do_change_perso"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylebluegray_900">
            <LinearLayout android:id="@+id/linearRowhome"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_7pxv"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackhome"
                    android:layout_height="@dimen/_39pxv"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginTop="@dimen/_7pxv" android:layout_width="@dimen/_34pxh">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtHome"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{frameSeventeenVM.frameSeventeenModel.txtHome}"
                        style="@style/txtInterregular12" tools:text="@string/lbl_home"/>
                    <ImageView android:id="@+id/imageVectorThree"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_width="@dimen/_29pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_amber_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_amber_900"/>
                </FrameLayout>
                <LinearLayout android:id="@+id/linearColumnvectorFour"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageVectorFour"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_width="@dimen/_34pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_amber_900_25x34"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_amber_900_25x34"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtFav"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{frameSeventeenVM.frameSeventeenModel.txtFav}"
                        style="@style/txtInterregular12" tools:text="@string/lbl_fav"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageVectorFive"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_48pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_width="@dimen/_52pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_amber_900_48x52"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_amber_900_48x52"/>
                <FrameLayout android:id="@+id/frameStackinbox"
                    android:layout_height="@dimen/_45pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_35pxh">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtInbox"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{frameSeventeenVM.frameSeventeenModel.txtInbox}"
                        style="@style/txtInterregular12" tools:text="@string/lbl_inbox"/>
                    <ImageView android:id="@+id/imageVectorSix"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_32pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_width="@dimen/_32pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_amber_900_32x32"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_amber_900_32x32"/>
                </FrameLayout>
                <LinearLayout android:id="@+id/linearColumnvectorSeven"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageVectorSeven"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_32pxh"
                        android:layout_width="@dimen/_32pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_32x32"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_32x32"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtAboutUs"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{frameSeventeenVM.frameSeventeenModel.txtAboutUs}"
                        style="@style/txtInterregular12" tools:text="@string/lbl_about_us"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
