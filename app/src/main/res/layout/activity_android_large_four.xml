<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="androidLargeFourVM" type="com.parveensapplication.app.modules.androidlargefour.data.viewmodel.AndroidLargeFourVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnvector"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_39pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_900">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearAndroidLarge"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:id="@+id/frameStackvector"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_140pxv"
                    android:layout_marginEnd="@dimen/_3pxh" android:layout_width="@dimen/_357pxh">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_gravity="top|end"
                        android:layout_height="@dimen/_93pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_21pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_black_900_18x15"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_black_900_18x15"/>
                    <FrameLayout
                        android:id="@+id/frameStackrectanglethirtysix"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_140pxv" android:layout_width="@dimen/_357pxh">
                        <ImageView
                            android:id="@+id/imageRectangleThirtySix"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_140pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_rectangle36}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle36"/>
                        <LinearLayout
                            android:id="@+id/linearColumnleadingmarketp"
                            android:layout_gravity="top|start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_29pxh"
                            android:layout_marginStart="@dimen/_29pxh"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <TextView android:gravity="start"
                                android:id="@+id/txtLeadingMarketp"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="match_parent"
                                android:text="@{androidLargeFourVM.androidLargeFourModel.txtLeadingMarketp}"
                                style="@style/txt" tools:text="@string/msg_leading_marketp"/>
                            <TextView android:gravity="end"
                                android:id="@+id/txtLanguage"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_width="match_parent"
                                android:text="@{androidLargeFourVM.androidLargeFourModel.txtLanguage}"
                                style="@style/txt_1" tools:text="@string/msg_trade_securly_w"/>
                        </LinearLayout>
                    </FrameLayout>
                </FrameLayout>
                <View android:background="@color/amber_301"
                    android:id="@+id/lineLineSixteen"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_25pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_19pxv" android:layout_width="@dimen/_3pxh"/>
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtPrice"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:text="@{androidLargeFourVM.androidLargeFourModel.txtPrice}"
                    style="@style/txtIntermedium17" tools:text="@string/msg_buy_gaming_acco"/>
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="match_parent"
                    android:text="@{androidLargeFourVM.androidLargeFourModel.txtDescription}"
                    style="@style/txtIntermedium17" tools:text="@string/msg_gamemart_is_a_p"/>
                <View android:background="@color/amber_301"
                    android:id="@+id/lineLineFifteen"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_25pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_3pxh"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtHOWITWORKS"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{androidLargeFourVM.androidLargeFourModel.txtHOWITWORKS}"
                    style="@style/txtIntermedium20" tools:text="@string/lbl_how_it_works"/>
